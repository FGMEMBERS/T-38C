<!-- A wrapper file for the T-38C 3D model -->

<PropertyList>
 <path>tallon.ac</path>
 
 <animation>
        <type>range</type>
        <min-m>0</min-m>
        <max-m>20000</max-m>
    </animation>

 <!-- MP sound call -->
  <sound><path>Aircraft/T-38C/Sounds/mp-sound.xml</path></sound>
 <nasal>
    <load>
   

      var self = cmdarg();

      var float_0 = self.getNode("sim/multiplay/generic/float[0]", 1);
      var float_1 = self.getNode("sim/multiplay/generic/float[1]", 1);
	  var float_3 = self.getNode("sim/multiplay/generic/float[3]", 1);
	  var float_4 = self.getNode("sim/multiplay/generic/float[4]", 1);
      
      var crew_door = self.getNode("instrumentation/doors/crew/position-norm", 1);
      var passenger_door = self.getNode("instrumentation/doors/passenger/position-norm", 1);
	 
	  var canopy = self.getNode("sim/model/canopy-positions/Canopy/position-norm", 1);
      crew_door.alias(float_0);
      passenger_door.alias(float_1);
	  
	  canopy.alias(float_4);
    </load>

    <unload>
     
      crew_door.unalias();
      passenger_door.unalias();
    </unload>
  </nasal>
  
   
  <!-- Transparent Surfaces for Rembrandt rendering -->
  
   
   <effect>
     <inherits-from>Effects/model-transparent</inherits-from>
      <object-name>rearglass</object-name>
	   <object-name>frontglass</object-name>
	   <object-name>windscreen</object-name>
	   <object-name>CPT-hud-screen</object-name>
	   
	   
    </effect>

  
  <!-- Solid Surfaces for Rembrandt rendering -->
 
 
<!-- cockpit bits -->
 <model>
 <name>PilotB</name>
   <path>Aircraft/T-38C/Models/pilots/pilot.xml</path>
   <offsets>
     <x-m>-3.2</x-m>
     <y-m>0.0</y-m>
     <z-m>0.4</z-m>
	 <pitch-deg>8</pitch-deg>
   </offsets>
 </model> 

<model>
<name>PilotA</name>
   <path>Aircraft/T-38C/Models/pilots/pilot.xml</path>
   <offsets>
