     <y-m>0.0</y-m>
     <z-m>0.34</z-m>
	  <pitch-deg>8</pitch-deg>
   </offsets>
 </model> 
 
 

<model>
   <path>Aircraft/T-38C/Models/Panel/cockpit.xml</path>
